* Misc
***************************************************************************************************
**************************************************************************************************/
* {
	selection-background-color: #000;

}

QWidget {
	background-color: #888;


}

QSizeGrip {
	background: qlineargradient(x1: 0.5, y1: 0.5, x2: 1, y2: 1, stop: 0 #888, stop: 1 #111);
	margin-top:0.2em;
	margin-left:0.2em;
	border-bottom: 1px solid #000;
	border-right: 1px solid #000;

	width: 16px;
	height: 16px;
}
/**************************************************************************************************
***************************************************************************************************
* Tabs
***************************************************************************************************
**************************************************************************************************/
QTabWidget::pane { /* The tab widget frame */
	border: 1px solid #666;
	border-radius: 4px;
}

QTabWidget::tab-bar {
	left: 5px; /* move to the right by 5px */
}

 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	border: 1px solid #222;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	min-width: 8ex;
	padding: 0.5em 1em;

	color:#eee;
}

QTabBar::tab:selected, QTabBar::tab:hover {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	color: #c58600;
}

QTabBar::tab:selected {
	border-color: #444;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #444, stop: 1 #111);
	color: #ff0;

}

QTabBar::tab:!selected {
}

QTabBar::tab:bottom {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	border: 1px solid #222;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	min-width: 8ex;
	padding: 0.5em 1em;

	color:#eee;
}

QTabBar::tab:selected:bottom, QTabBar::tab:hover:bottom {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #444, stop: 1 #222);
	color: #c58600;
}

QTabBar::tab:selected:bottom {
	border-color: #444;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	color: #ff0;

}

QTabBar::tab:!selected:bottom {
}

/**************************************************************************************************
***************************************************************************************************
* Inputs
***************************************************************************************************
**************************************************************************************************/
QAbstractScrollArea, QComboBox, QLineEdit, QDateEdit, QDateTimeEdit, QTextEdit, QTimeEdit, QSpinBox, QDoubleSpinBox {
	background-color: #000;
	alternate-background-color:#333;
	color: #ddd;
	border: 0px solid #222;
/*	border-radius: 0em; */
	padding: 0.1em 0.4em 0.1em 0.4em;
	background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	border-radius:4px;
	border: 1px solid #000;
}	


/* Drop down lists */
QComboBox {
	background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	border-radius:4px;
	min-width: 6em;
}

QComboBox:editable {
	background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
}

QComboBox:!editable, QComboBox::drop-down:editable {
	background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	color:#f00;
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
}

QComboBox:on { /* shift the text when the popup opens */
	padding-top: 3px;
	padding-left: 4px;
}

QComboBox::drop-down {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #888, stop: 1 #111);
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 1em;

	border-left-width: 1px;
	border-left-color: #222;
	border-left-style: solid; /* just a single line */
	border-top-right-radius: 3px; /* same radius as the QComboBox */
	border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
}




QWidget#qteRichText
{
	background: #fff;
	color:#000;
}


/* Buttons */

QPushButton {
	background-color: #000;
	color: #c58600;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	padding:0.5em 2em;
	border:1px solid #000;
	margin:0.4em;
}

QPushButton::pressed {
	background-color: #000;
	color: #c58600;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	padding:0.5em;

}

QPushButton::hover {
	background-color: #000;
	color: #c58600;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #444, stop: 1 #222);
	padding:0.5em;
	border:1px solid #222;

}
QPushButton::default {
	background-color: #000;
	color: #86c500;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);

}
QPushButton::default::hover {
	background-color: #000;
	color: #86c500;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #444, stop: 1 #222);
	padding:0.5em;

}

QPushButton::flat {
	background-color: #000;
	color: #f88;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);

}
QPushButton::checked {
	background-color: #000;
	color: #ff0;
	border-radius: 4px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);

}




