/**************************************************************************************************
***************************************************************************************************
* MenuBar
***************************************************************************************************
**************************************************************************************************/

QMenuBar {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	background: #000;
	color: #c58600;
	font-weight: bold;
	border: 1px solid black;
	border-bottom: 1px solid #222;
}

QMenuBar::item {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	color: #c58600;
	padding: 4px 8px;
	border: 1px solid transparent;
	spacing: 1em; /* spacing between menu bar items */
	border-radius: 4px;

}

QMenuBar::item:selected {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #222, stop: 1 #000);
	color: #c58600;
}

QMenuBar::item:pressed {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #222, stop: 1 #000);
	color: #ff0;
}

/**************************************************************************************************
***************************************************************************************************
* Toolbar horizontal 
