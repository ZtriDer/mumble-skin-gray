	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
/*
	spacing: 3px; 
*/
	border-radius: 0px; /* if omitted produces weird white border */
	border: 0px solid #222;

}

QToolBar:vertical {
	background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #333, stop: 1 #111);
/*
	spacing: 3px; 
*/
	border-radius: 0px; /* if omitted produces weird white border */
	border: 0px;
}


QToolBar::handle {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #222, stop: 1 #000);
	border: 1px solid #000;
}
QToolBar::separator:vertical {
	border: 1px solid transparent;
}
QToolBar::separator:horizontal {
	border-left: 1px solid #000;
}



QToolButton {
