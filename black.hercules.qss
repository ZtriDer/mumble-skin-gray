/**************************************************************************************************
***************************************************************************************************
* Scrollbars : Vertical
***************************************************************************************************
**************************************************************************************************/

QScrollBar {
	background: #333;
	width: 1em;
	margin:0 0;
	border-radius: 4px;
}

QScrollBar:vertical {
	background: #333;
	width: 1em;
	margin:0 0;
	border-radius: 4px;
}

QScrollBar::handle:vertical {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #222, stop:1 #666);
	min-height: 0.5em;
	border-radius: 4px;
	border: 1px solid #000;

}

QScrollBar::handle:vertical:hover {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #222, stop:1 #666);
}

QScrollBar::add-line:vertical {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #666, stop:1 #222);
	height: 0.5em;
	subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #666, stop:1 #222);
	height: 0.5em;
	subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover, QScrollBar::sub-line:vertical:hover {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #222, stop:1 #666);
}

QScrollBar::add-line:vertical {
	subcontrol-position: bottom;
}

QScrollBar::sub-line:vertical {
	subcontrol-position: top;
}

/**************************************************************************************************
***************************************************************************************************
* Scrollbars : Horizontal
***************************************************************************************************
**************************************************************************************************/
QScrollBar:horizontal {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #666, stop:1 #000);
	height: 1em;
	margin: 0 1.2em;
}

QScrollBar::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6666, stop:1 #222);
	min-width: 0.5em;
}

QScrollBar::handle:horizontal:hover {
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #222, stop:1 #666);
}

QScrollBar::add-line:horizontal {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #666, stop:1 #222);
	width: 0.5em;
	subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #666, stop:1 #222);
	width: 0.5em;
	subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover, QScrollBar::sub-line:horizontal:hover {
	border: 1px solid #000;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #222, stop:1 #666);
}

QScrollBar::add-line:horizontal {
	subcontrol-position: right;
}

QScrollBar::sub-line:horizontal {
	subcontrol-position: left;
}


