	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	color: #ff0;
	border: 1px solid #000;
}

QMenu::item:pressed {
	background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #111);
	color: #ff0;
	border: 1px solid #000;
}

QMenu::item:disabled {
	color: #888;
}

QMenu::item:disabled:selected {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 1 #111);
	color: #f00;
	border: 1px solid #000;
}

QMenu::separator {
	background: #222;
	height: 1px;
	border-top: 1px solid #000;
}

QMenu::indicator {
	width: 13px;
	height: 13px;
	padding-left: 2px;
}


